{
  "globalDependencies": ["$SANITY_API_TOKEN", "$NEXT_PUBLIC_SANITY_DATASET", "$NEXT_PUBLIC_SANITY_PROJECT_ID", "$NODE_ENV"],
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "export": {
      "dependsOn": ["^build"],
      "outputs": ["out/**"]
    }
  }
}
